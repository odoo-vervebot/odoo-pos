<odoo>
  <data>
    <!-- explicit list view definition -->
    <record model="ir.ui.view" id="label_print.list">
      <field name="name">label_print list</field>
      <field name="model">label_print.label_print</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="value"/>
          <field name="value2"/>
        </tree>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="label_print.action_window">
      <field name="name">label_print window</field>
      <field name="res_model">label_print.label_print</field>
      <field name="view_mode">tree,form</field>
    </record>


    <!-- server action to the one above -->

    <record model="ir.actions.server" id="label_print.action_server">
      <field name="name">label_print server</field>
      <field name="model_id" ref="model_label_print_label_print"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": model._name,
        }
      </field>
    </record>


    <!-- Top menu item -->

    <menuitem name="label_print" id="label_print.menu_root"/>

    <!-- menu categories -->

    <menuitem name="Menu 1" id="label_print.menu_1" parent="label_print.menu_root"/>
    <menuitem name="Menu 2" id="label_print.menu_2" parent="label_print.menu_root"/>

    <!-- actions -->

    <menuitem name="List" id="label_print.menu_1_list" parent="label_print.menu_1"
              action="label_print.action_window"/>
 <menuitem name="Server to list" id="label_print" parent="label_print.menu_2"
              action="label_print.action_server"/>

  </data>
</odoo>